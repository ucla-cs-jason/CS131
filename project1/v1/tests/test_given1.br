func main() {  /* a function that computes the sum of two numbers */
  var first;
  var second;
  first = inputi("Enter a first #: ");
  second = inputi("Enter a second #: ");
  var sum;
  sum = (first + second);   
  print("The sum is ", sum, "!");
}
