struct Person {
  name: string;
  age: int;
  student: bool;
}

func main() : void {
  var p: Person;
  p = new Person;
  p.name = "Carey";
  p.age = 21;
  p.student = false;
  foo(p);
}

func foo(p : Person) : void {
  print(p.name, " is ", p.age, " years old.");
}

/*
*OUT*
Carey is 21 years old.
*OUT*
*/