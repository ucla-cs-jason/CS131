func main(): void {
  print(true || false);  /* prints true */
  print(true || false && false); /* prints true */
  print(5/3);            /* prints 1 */
  print(-6);             /* prints -6 */
  print(!true);          /* prints false */

  var a: int;
  a = 3;
  print(a > 5);          /* prints false */
  print("abc"+"def");    /* prints abcdef */
}



